{
    "name": "simple-as-fuck/php-api-toolkit",
    "require": {
        "php": "^7.4|^8.0",
        "ext-json": "*",
        "psr/http-message": "^1.0",
        "psr/http-factory": "^1.0",
        "simple-as-fuck/php-validator": "^0.3.0|^0.4.0",
        "guzzlehttp/guzzle": "^7.4",
        "illuminate/support": "^8.81|^9.0",
        "kayex/http-codes": "^1.1",
        "guzzlehttp/psr7": "^2.1",
        "symfony/psr-http-message-bridge": "^2.1",
        "symfony/http-foundation": "^5.4|^6.0",
        "symfony/http-kernel": "^5.4|^6.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.2",
        "friendsofphp/php-cs-fixer": "^3.4",
        "phpunit/phpunit": "^9.5",
        "phpstan/phpstan-strict-rules": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0"
    },
    "autoload": {
        "psr-4": {"SimpleAsFuck\\ApiToolkit\\": "src/"}
    },
    "extra": {
        "laravel": {
            "providers": [
                "SimpleAsFuck\\ApiToolkit\\Provider\\LaravelProvider"
            ]
        }
    }
}
